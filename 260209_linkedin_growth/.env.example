# LinkedIn OAuth 2.0
LINKEDIN_CLIENT_ID=your_client_id_here
LINKEDIN_CLIENT_SECRET=your_client_secret_here
LINKEDIN_REDIRECT_URI=http://localhost:8080/callback

# LinkedIn 사용자 ID
# - 권장: OAuth로 확인한 URN (예: urn:li:person:XXXXX)
# - 임시: 핸들(sanguinekim)도 가능 (collector에서 URN으로 변환 시도)
# 프로필 URL: https://www.linkedin.com/in/sanguinekim/
LINKEDIN_USER_ID=

# Mock 모드
# - 실데이터 연동 시 false
USE_MOCK_DATA=true

# 토큰 암호화 키 (Fernet, 자동 생성됨)
# FERNET_KEY=

# Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# Ollama 로컬 폴백 (선택)
# OLLAMA_HOST=http://localhost:11434
# OLLAMA_MODEL=qwen2.5:14b
